<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainMenuProfileFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="101dp"
        android:background="#E0FFFF"
        android:orientation="vertical">

    </LinearLayout>
    <TextView
        android:id="@+id/myInfoTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_info"
        android:textSize="24sp"
        android:textStyle="bold"
        android:fontFamily="@font/kotrahope"
        android:layout_marginTop="10dp"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/usernameTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="userName"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/kotrahope"/>

    <TextView
        android:id="@+id/sirTv"
        android:layout_width="44dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/kotrahope"
        android:text="@string/sir"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        android:layout_marginLeft="120dp"

        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/name"
        android:textSize="18sp"
        android:layout_marginTop="120dp"
        android:textColor="#a9a9a9"
        android:fontFamily="@font/kotrahope"/>

    <TextView
        android:id="@+id/usernameTv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="18sp"
        android:fontFamily="@font/kotrahope"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="100dp"
        android:textColor="#a9a9a9"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/id"
        android:textSize="18sp"
        android:layout_marginTop="160dp"
        android:fontFamily="@font/kotrahope"
        android:textColor="#a9a9a9"/>

    <TextView
        android:id="@+id/useridTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="18sp"
        android:layout_marginTop="160dp"
        android:layout_marginLeft="100dp"
        android:textColor="#a9a9a9"
        android:fontFamily="@font/kotrahope"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/email"
        android:textSize="18sp"
        android:layout_marginTop="200dp"
        android:textColor="#a9a9a9"
        android:fontFamily="@font/kotrahope"
        />

    <TextView
        android:id="@+id/useremailTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="18sp"
        android:fontFamily="@font/kotrahope"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="100dp"
        android:textColor="#a9a9a9"/>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/gender"
        android:textSize="18sp"
        android:textColor="#a9a9a9"
        android:layout_marginTop="240dp"
        android:fontFamily="@font/kotrahope"/>

    <TextView
        android:id="@+id/usergenderTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="18sp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="240dp"
        android:textColor="#a9a9a9"
        android:fontFamily="@font/kotrahope"/>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="280dp"
        android:text="@string/years"
        android:textSize="18sp"
        android:textColor="#a9a9a9"
        android:fontFamily="@font/kotrahope"/>

    <TextView
        android:id="@+id/useryearsTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="280dp"
        android:textSize="18sp"
        android:layout_marginLeft="100dp"
        android:fontFamily="@font/kotrahope"
        android:textColor="#a9a9a9"/>

    <TextView
        android:id="@+id/logoutTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/logout"
        android:textColor="#a9a9a9"
        android:layout_marginTop="500dp"
        android:layout_marginLeft="300dp"
        android:fontFamily="@font/kotrahope"
        android:textSize="20dp"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="95dp"
        android:layout_height="88dp"
        android:layout_marginLeft="300dp"
        android:src="@drawable/person" />


</FrameLayout>